# Define ':' for sequencing: as a low-precedence operator that ignores operands
# and just returns the RHS.
. binary : 1 (x y) y;

. newtest(x)
  var y = x in
    (y = (y * 10)): 
  y;

. testIfFiveTenElse(x)
  if (x < 5) then
    1
  else
    if(x < 101) then
      2
    else 
      0;
      
. eenewesttest(x)
  if (x <= 2) then
    x
  else
    1;



